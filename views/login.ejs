                <div class="input-field col s12">
                    <i class="material-icons prefix">email</i>
                    <input id="email" type="email" class="validate" placeholder="E mail">
                    <label for="email">E mail</label>
                </div>
                <div class="input-field col s12">
                    <i class="material-icons prefix">Message</i>
                    <input id="meaasge" type="text" class="validate" placeholder="Your suggestion here">
                    <label for="message">Message</label>
                </div>
                <div class="input-field col s12">
                    <i class="material-icons prefix">dialpad</i>
                    <input id="servings" type="number" class="validate" placeholder="Your rating for this platform out of 10">
                    <label for="servings">Your rating (__/10)</label>
                </div>
                <a href="#" id="download-button" class="btn-large waves-effect waves-light amber darken-3">Submit</a>
            </form>
